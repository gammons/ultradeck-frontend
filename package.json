{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "highcharts": "^7.1.2",
    "highlight.js": "^10.4.1",
    "markdown-it": "^9.0.1",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-link-attributes": "^2.1.0",
    "node-emoji": "^1.10.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm-run-all build-css build-js",
    "build-js": "react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/styles/ -o src/css/ --watch --recursive",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "run-storybook": "start-storybook -p 9009 -s .storybook/public",
    "storybook": "npm-run-all -p watch-css run-storybook",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "eslint-plugin-prettier": "^3.1.0",
    "flow-bin": "^0.104.0",
    "prettier": "^1.18.2"
  }
}
